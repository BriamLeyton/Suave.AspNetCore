{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "Startup.fs",
        "Program.fs"
      ]
    }
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "publishOptions": {
    "include": [
      "web.config"
    ]
  },
  "dependencies": {
    "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-161205",
    "Microsoft.AspNetCore.Diagnostics": "1.1.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.0",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Suave": "2.0.0-rc8",
    "Suave.AspNetCore": "1.0.0",
    "Suave.DotLiquid": "2.0.0-rc8",
    "Newtonsoft.Json": "9.0.1",
    "DotLiquid": "2.0.64"
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-020000",
      "imports": [
        "dnxcore50",
        "portable-net45+win81",
        "netstandard1.3"
      ]
    }
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      },
      "imports": "dnxcore50"
    }
  }
}